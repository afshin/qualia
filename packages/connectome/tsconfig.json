{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "lib": ["DOM", "ES2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ES2015",
    "types": []
  },
  "references": [
    {
      "path": "../graph"
    },
    {
      "path": "../neuron"
    }
  ]
}
